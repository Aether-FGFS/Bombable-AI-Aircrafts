<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>mainrotor.ac</path>

  <animation>
    <object-name>rotor1</object-name>
    <object-name>rotor2</object-name>
    <object-name>rotor3</object-name>
    <object-name>rotor4</object-name>
    <object-name>rotor5</object-name>
    <object-name>rotor6</object-name>
    <object-name>rotor7</object-name>
    <object-name>rotor8</object-name>
    <object-name>rotor9</object-name>
    <object-name>rotor10</object-name>
    <object-name>rotor11</object-name>
    <object-name>rotor12</object-name>
    <object-name>rotor13</object-name>
    <object-name>rotor14</object-name>
    <object-name>rotor15</object-name>
    <object-name>rotor16</object-name>
    <object-name>rotor17</object-name>
    <object-name>rotor18</object-name>
    <object-name>rotor19</object-name>
  </animation>

  <!-- Blades -->
  <model>
    <name>Blade1</name>
    <path>blade1.xml</path>
    <offsets>
      <x-m> 0.000 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.594 </z-m>
    </offsets>
  </model>

  <model>
    <name>Blade2</name>
    <path>blade2.xml</path>
    <offsets>
      <x-m> 0.000 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.594 </z-m>
      <heading-deg> 180 </heading-deg>
    </offsets>
  </model>

  <animation>
    <name>rotorcomplet</name>
    <object-name>rotor5</object-name>
    <object-name>Blade1</object-name>
    <object-name>Blade2</object-name>
  </animation>

  <animation>
    <name>Ensemble</name>
    <object-name>rotorcomplet</object-name>
    <object-name>rotor3</object-name>
    <object-name>rotor4</object-name>
    <object-name>rotor5</object-name>
    <object-name>rotor6</object-name>
    <object-name>rotor7</object-name>
    <object-name>rotor8</object-name>
    <object-name>rotor9</object-name>
    <object-name>rotor10</object-name>
    <object-name>rotor11</object-name>
    <object-name>rotor12</object-name>
    <object-name>rotor13</object-name>
    <object-name>rotor14</object-name>
    <object-name>rotor15</object-name>
    <object-name>rotor16</object-name>
    <object-name>rotor17</object-name>
    <object-name>rotor18</object-name>
    <object-name>rotor19</object-name>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>Ensemble</object-name>
    <property>velocities/true-airspeed-kt</property>
    <factor> 100 </factor>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

</PropertyList>
