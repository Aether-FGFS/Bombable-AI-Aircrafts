<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>mainrotor.ac</path>

  <animation>
    <object-name>rotor</object-name>
    <object-name>rotorup</object-name>
  </animation>

  <!-- Blades -->
  <model>
    <name>Blade1</name>
    <path>blade.xml</path>
  </model>

  <model>
    <name>Blade2</name>
    <path>blade.xml</path>
    <offsets>
      <heading-deg> 90 </heading-deg>
    </offsets>
  </model>

  <model>
    <name>Blade3</name>
    <path>blade.xml</path>
    <offsets>
      <heading-deg> 180 </heading-deg>
    </offsets>
  </model>

  <model>
    <name>Blade3</name>
    <path>blade.xml</path>
    <offsets>
      <heading-deg> 270 </heading-deg>
    </offsets>
  </model>

  <animation>
    <name>rotorcomplet</name>
    <object-name>rotorup</object-name>
    <object-name>Blade1</object-name>
    <object-name>Blade2</object-name>
    <object-name>Blade3</object-name>
    <object-name>Blade4</object-name>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>Ensemble</object-name>
    <property>velocities/true-airspeed-kt</property>
    <factor> 100 </factor>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

</PropertyList>
