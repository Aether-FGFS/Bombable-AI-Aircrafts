<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>tailrotor.ac</path>

  <animation>
    <object-name>tailrotor</object-name>
    <object-name>tailrotor2</object-name>
    <object-name>tailrotor3</object-name>
    <object-name>tailrotor4</object-name>
    <object-name>tailrotor5</object-name>
  </animation>

  <!-- Blades -->
  <model>
    <name>Blade1</name>
    <path>blade.xml</path>
    <offsets>
      <z-m>  0.096 </z-m>
      <heading-deg> -90 </heading-deg>
    </offsets>
  </model>

  <model>
    <name>Blade2</name>
    <path>blade.xml</path>
    <offsets>
      <z-m>  0.096 </z-m>
      <heading-deg> -210 </heading-deg>
    </offsets>
  </model>

  <model>
    <name>Blade3</name>
    <path>blade.xml</path>
    <offsets>
      <z-m>  0.096 </z-m>
      <heading-deg> -330 </heading-deg>
    </offsets>
  </model>

  <animation>
    <name>rotorcomplet</name>
    <object-name>tailrotor</object-name>
    <object-name>tailrotor2</object-name>
    <object-name>tailrotor3</object-name>
    <object-name>tailrotor4</object-name>
    <object-name>tailrotor5</object-name>
    <object-name>Blade1</object-name>
    <object-name>Blade2</object-name>
    <object-name>Blade3</object-name>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>rotorcomplet</object-name>
    <property>velocities/true-airspeed-kt</property>
    <factor> 100 </factor>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

</PropertyList>
