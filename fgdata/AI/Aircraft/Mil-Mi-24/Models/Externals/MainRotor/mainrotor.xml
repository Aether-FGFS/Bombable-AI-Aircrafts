<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>mainrotor.ac</path>

  <animation>
    <object-name>rotor</object-name>
  </animation>

  <!-- Blades -->
  <model>
    <name>Blade1</name>
    <path>blade1.xml</path>
  </model>

  <model>
    <name>Blade2</name>
    <path>blade2.xml</path>
    <offsets>
      <heading-deg> 72 </heading-deg>
    </offsets>
  </model>

  <model>
    <name>Blade3</name>
    <path>blade3.xml</path>
    <offsets>
      <heading-deg> 144 </heading-deg>
    </offsets>
  </model>

  <model>
    <name>Blade4</name>
    <path>blade4.xml</path>
    <offsets>
      <heading-deg> 216 </heading-deg>
    </offsets>
  </model>

  <model>
    <name>Blade5</name>
    <path>blade5.xml</path>
    <offsets>
      <heading-deg> 288 </heading-deg>
    </offsets>
  </model>

  <animation>
    <name>touteslespales</name>
    <object-name>Blade1</object-name>
    <object-name>Blade2</object-name>
    <object-name>Blade3</object-name>
    <object-name>Blade4</object-name>
    <object-name>Blade5</object-name>
  </animation>

  <animation>
    <name>rotorcomplet</name>
    <object-name>rotor</object-name>
    <object-name>Blade1</object-name>
    <object-name>Blade2</object-name>
    <object-name>Blade3</object-name>
    <object-name>Blade4</object-name>
    <object-name>Blade5</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>rotorcomplet</object-name>
    <property>velocities/true-airspeed-kt</property>
    <factor> 10 </factor>
    <axis>
      <x>  0 </x>
      <y>  0 </y>
      <z> -1 </z>
    </axis>
  </animation>

</PropertyList>
